{
    "service_name" : "${LAMBDA_SERVICE_NAME}",
    "function_name" : "${LAMBDA_FUNCTION_NAME}",
    "description" : "${LAMBDA_FUNCTION_DESCRIPTION}",
    "role" : "${local.common.project_code}-${local.common.service_name}-${LAMBDA_FUNCTION_NAME}-lambda-role-${local.common.environment}",
    "handler" : "${LAMBDA_HANDLER}",
    "runtime" : "${LAMBDA_RUNTIME}",
    "memory_size" : "${LAMBDA_MEMORY_SIZE}",
    "timeout" : "${LAMBDA_TIMEOUT}",
    "custom_layers" : "${LAMBDA_CUSTOM_LAYERS}",
    "vpc_config" : {
        "subnet_ids"         : "${LAMBDA_VPC_SUBNET_IDS}",
        "security_group_ids" : "${LAMBDA_VPC_SECURITY_GROUP_IDS}"
    },
    "variables" : "${LAMBDA_VARIABLES}",
    "explicit_service_policies": "${LAMBDA_EXPLICIT_SERVICE_POLICIES}"
}