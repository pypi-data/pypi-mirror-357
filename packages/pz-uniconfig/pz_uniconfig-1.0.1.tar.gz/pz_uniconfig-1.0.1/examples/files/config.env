# .env example for ConfigManager testing

APP_NAME="dotenv-demo"
ENV=production
DEBUG=false
VERSION=3.2.1

PLOTTWIST.CATCHY.PROB=yeah

# Numbers (still strings in dotenv files)
MAX_USERS=250
TIMEOUT_SECONDS=7.5

# Database config
DB_HOST=localhost
DB_PORT=5432
DB_NAME=myapp_db
DB_USER=poziel
DB_PASS="super_secret_pass"

# API keys & tokens
GOOGLE_API_KEY=AIzaSyA...fake
GITHUB_TOKEN=ghp_abcdefg1234567890

# Multi-value: supported via comma (but still a string in dotenv)
ALLOWED_ORIGINS="https://poziel.com,https://another.com,http://localhost:8080"

# Boolean (must be parsed in your app logic)
EMAIL_VERIFICATION=true
ALLOW_GUESTS=no

# Paths with spaces and special chars
LOG_PATH="/var/log/myapp logs/app.log"
UPLOADS_DIR=C:\\Uploads\\Test

# Quoted values, single and double
MESSAGE='Bienvenue chez Poziel!'
WELCOME_BANNER="Welcome to the dotenv test, Poziel!"

# Empty values
OPTIONAL_FEATURE=

# Null/None-like value (string 'null', your code can handle as needed)
SUSPENDED=null

# Edge case: comments and spaces
# This is a comment line
   SPACEY_KEY = "  value with leading and trailing spaces  "

# Feature flags
FEATURE_CHAT=1
FEATURE_LEADERBOARD=0

# Nested-like notation (dotenv doesn't do nesting, but your lib could handle it via parsing)
SERVER__EU__HOST=eu.example.com
SERVER__EU__PORT=8080
SERVER__US__HOST=us.example.com
SERVER__US__PORT=8081

# Final line