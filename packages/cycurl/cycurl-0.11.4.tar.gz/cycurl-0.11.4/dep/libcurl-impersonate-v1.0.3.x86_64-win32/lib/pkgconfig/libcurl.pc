prefix=C:/a/curl-impersonate/curl-impersonate/packages
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
supported_protocols="DICT FILE FTP FTPS GOPHER GOPHERS HTTP HTTPS IMAP IMAPS IPFS IPNS LDAP LDAPS MQTT POP3 POP3S RTSP SMB SMBS SMTP SMTPS TELNET TFTP WS WSS"
supported_features="alt-svc AsynchDNS brotli ECH HSTS HTTP2 HTTP3 HTTPS-proxy HTTPSRR IDN IPv6 Largefile libz NTLM SSL threadsafe Unicode UnixSockets zstd"

Name: libcurl
URL: https://curl.se/
Description: Library to transfer files with HTTP, FTP, etc.
Version: 8.13.0-IMPERSONATE
Requires: 
Requires.private: openssl,zlib,libbrotlidec,libbrotlicommon,libzstd,libnghttp2,libngtcp2,libngtcp2_crypto_boringssl,libnghttp3
Libs: -L${libdir} -lcurl-impersonate 
Libs.private:  C:/a/curl-impersonate/curl-impersonate/packages/lib/ssl.lib C:/a/curl-impersonate/curl-impersonate/packages/lib/crypto.lib C:/a/curl-impersonate/curl-impersonate/packages/lib/zlib.lib C:/a/curl-impersonate/curl-impersonate/packages/lib/brotlidec.lib C:/a/curl-impersonate/curl-impersonate/packages/lib/brotlicommon.lib C:/a/curl-impersonate/curl-impersonate/packages/lib/zstd.lib C:/a/curl-impersonate/curl-impersonate/packages/lib/nghttp2.lib C:/a/curl-impersonate/curl-impersonate/packages/lib/ngtcp2.lib C:/a/curl-impersonate/curl-impersonate/packages/lib/ngtcp2_crypto_boringssl.lib C:/a/curl-impersonate/curl-impersonate/packages/lib/nghttp3.lib -lwldap32 -lnormaliz -lws2_32 -lbcrypt -ladvapi32 -lcrypt32
Cflags: -I${includedir} 
Cflags.private: -DCURL_STATICLIB
