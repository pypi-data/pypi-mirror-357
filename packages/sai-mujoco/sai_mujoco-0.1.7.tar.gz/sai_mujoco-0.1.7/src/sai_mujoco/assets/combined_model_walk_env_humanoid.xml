<mujoco><statistic center="0.3 0 0.35" extent="0.8" meansize="0.05" />
    <compiler angle="radian" autolimits="true" />
    <option integrator="implicitfast" />

    <visual>
        <global offwidth="2560" offheight="1920" />
        <quality shadowsize="4096" offsamples="8" />
        <map force="0.1" fogend="5" />
        <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0" />
        <rgba haze="0.15 0.25 0.35 1" />
        <scale contactwidth="0.075" contactheight="0.025" forcewidth="0.05" com="0.05" framewidth="0.01" framelength="0.2" />
    </visual>

    <include file="./robots/humanoid/assets.xml" />
    <include file="./robots/humanoid/actuators.xml" />
    
    <asset><texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072" />
        <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3" markrgb="0.8 0.8 0.8" width="300" height="300" />
        <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2" />
    </asset><worldbody><light pos="0 0 1.5" dir="0 0 -1" directional="true" />

        <body name="floor">
			<geom contype="1" conaffinity="2" friction=".1 0.005 0.0001" name="floor" pos="0 0 0" size="0 0 .25" type="plane" material="groundplane" condim="3" solimp="0 0.95 0.001 0.5 2" solref="0.02 1" margin="0" />
		</body>
        <camera name="main_cam" pos="1.834 1.700 1.927" xyaxes="-0.742 0.670 -0.000 -0.380 -0.421 0.823" />

    <include file="./robots/humanoid/model.xml" />
    </worldbody><sensor><gyro site="imu" name="gyro" />
        <velocimeter site="imu" name="local_linvel" />
        <accelerometer site="imu" name="accelerometer" />
        <framezaxis objtype="site" objname="imu" name="upvector" />
        <framexaxis objtype="site" objname="imu" name="forwardvector" />
        <framelinvel objtype="site" objname="imu" name="global_linvel" />
        <frameangvel objtype="site" objname="imu" name="global_angvel" />
        <framepos objtype="site" objname="imu" name="position" />
        <framequat objtype="site" objname="imu" name="orientation" />

        <framelinvel objtype="site" objname="left_foot" name="left_foot_global_linvel" />
        <framelinvel objtype="site" objname="right_foot" name="right_foot_global_linvel" />
        <framepos objtype="site" objname="left_foot" name="left_foot_pos" />
        <framepos objtype="site" objname="right_foot" name="right_foot_pos" />
    </sensor></mujoco>