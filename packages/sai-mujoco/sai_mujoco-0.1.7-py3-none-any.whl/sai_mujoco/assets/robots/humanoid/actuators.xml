<mujocoinclude>
    <tendon>
        <fixed name="left_hipknee">
            <joint coef="-1" joint="robot:left_hip_y"/>
            <joint coef="1" joint="robot:left_knee"/>
        </fixed>
        <fixed name="right_hipknee">
            <joint coef="-1" joint="robot:right_hip_y"/>
            <joint coef="1" joint="robot:right_knee"/>
        </fixed>
    </tendon>

    <actuator>
        <motor gear="100" joint="robot:abdomen_y" name="abdomen_y"/>
        <motor gear="100" joint="robot:abdomen_z" name="abdomen_z"/>
        <motor gear="100" joint="robot:abdomen_x" name="abdomen_x"/>
        <motor gear="100" joint="robot:right_hip_x" name="right_hip_x"/>
        <motor gear="100" joint="robot:right_hip_z" name="right_hip_z"/>
        <motor gear="300" joint="robot:right_hip_y" name="right_hip_y"/>
        <motor gear="200" joint="robot:right_knee" name="right_knee"/>
        <motor gear="100" joint="robot:left_hip_x" name="left_hip_x"/>
        <motor gear="100" joint="robot:left_hip_z" name="left_hip_z"/>
        <motor gear="300" joint="robot:left_hip_y" name="left_hip_y"/>
        <motor gear="200" joint="robot:left_knee" name="left_knee"/>
        <motor gear="25" joint="robot:right_shoulder1" name="right_shoulder1"/>
        <motor gear="25" joint="robot:right_shoulder2" name="right_shoulder2"/>
        <motor gear="25" joint="robot:right_elbow" name="right_elbow"/>
        <motor gear="25" joint="robot:left_shoulder1" name="left_shoulder1"/>
        <motor gear="25" joint="robot:left_shoulder2" name="left_shoulder2"/>
        <motor gear="25" joint="robot:left_elbow" name="left_elbow"/>
    </actuator>
</mujocoinclude>